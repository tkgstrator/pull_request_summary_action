{
  "name": "pull_request_summary_action",
  "version": "0.0.0",
  "private": true,
  "description": "Update Pull Request Summary by ChatGPT",
  "main": "lib/main.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "swc src -d lib",
    "format": "prettier --write '**/*.ts'",
    "format:fix": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "author": "@tkgling",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.12",
    "openai": "^4.33.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@commitlint/cz-commitlint": "^18.6.0",
    "@types/node": "^20.1.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vercel/ncc": "^0.38.1",
    "commitizen": "^4.3.0",
    "commitlint": "^18.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^28.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "swc": "^1.0.11",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}